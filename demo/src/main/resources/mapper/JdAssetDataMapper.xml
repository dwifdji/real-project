<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatisPlus.demo.mapper.JdAssetDataMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mybatisPlus.demo.model.JdAssetData">
        <id column="id" property="id" />
        <result column="code" property="code" />
        <result column="name" property="name" />
        <result column="province" property="province" />
        <result column="city" property="city" />
        <result column="country" property="country" />
        <result column="address" property="address" />
        <result column="build_area" property="buildArea" />
        <result column="category_name" property="categoryName" />
        <result column="lng" property="lng" />
        <result column="lat" property="lat" />
        <result column="create_time" property="createTime" />
        <result column="delete_flag" property="deleteFlag" />
        <result column="current_price" property="currentPrice" />
        <result column="start_price" property="startPrice" />
        <result column="assessment_price" property="assessmentPrice" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, code, name, province, city, country, address, build_area, category_name, lng, lat, create_time, delete_flag, current_price, start_price, assessment_price
    </sql>

</mapper>
